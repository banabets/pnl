{
  "name": "ape-of-the-hill-volume-bot",
  "version": "1.0.0",
  "description": "Ape Of The Hill Volume Bot - A secure and transparent Solana volume bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "cd web && npm install && npm run build",
    "build:web": "cd web && npm install && npm run build",
    "build:server": "tsc",
    "build:full": "tsc && npm run build:web",
    "start": "node dist/server/index.js",
    "start:server": "node dist/server/index.js",
    "start:web": "node dist/server/index.js",
    "dev": "ts-node src/index.ts",
    "dev:web": "ts-node server/index.ts",
    "simulate": "ts-node src/simulate.ts",
    "clean": "rm -rf dist",
    "vercel-build": "cd web && npm install && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration"
  },
  "keywords": [
    "solana",
    "raydium",
    "volume",
    "trading",
    "defi",
    "ape",
    "hill"
  ],
  "author": "Ape Of The Hill Team",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.47",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.91.7",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^5.1.1",
    "bn.js": "^5.2.1",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "inquirer": "^8.2.6",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.21.0",
    "socket.io": "^4.7.2",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4",
    "@sentry/node": "^7.91.0",
    "@sentry/profiling-node": "^1.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bn.js": "^5.1.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.2.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "socket.io-client": "^4.8.3",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "ws": "^8.18.3"
  }
}
